<section class="blog-area blog-bg pt-100 pb-100">
    <div class="container">
        <div class="row blog-active">
            @foreach (var item in Model.blogSlider)
            {
                <div class="col-lg-4">
                    <div class="single-blog">
                        @if (item.BlogPhotos[0].PhotoUrl != null)
                        {
                            <div class="blag-image">
                                <a asp-controller="Blog" asp-action="Detail" asp-route-id="@item.Id"><img src="~/assets/images/blog/@item.BlogPhotos[0].PhotoUrl" alt="blog"></a>
                                <p class="meta-date">
                                    <span class="day">05</span>
                                    <span class="month">Dec</span>
                                </p>
                            </div>
                        }
                        @if (item.BlogPhotos[0].VideoUrl != null)
                        {
                            <div class="blag-image">
                                <a asp-controller="Blog" asp-action="Detail" asp-route-id="@item.Id">
                                    <iframe src="@item.BlogPhotos[0].VideoUrl" allow="autoplay; encrypted-media" allowfullscreen="" ></iframe>
                                </a>
                                <p class="meta-date">
                                    <span class="day">05</span>
                                    <span class="month">Dec</span>
                                </p>
                            </div>
                        }
                        <div class="blag-content">
                            <h2 class="blog-title"><a href="blog-single-left-sidebar.html">@item.Title</a></h2>
                            <p>
                                @{
                                    string ShortDesc = item.Description;
                                    if (ShortDesc.Length > 75)
                                    {
                                        ShortDesc = ShortDesc.Substring(0, 75) + "...";
                                    }
                                }
                                @ShortDesc
                            </p>
                        </div>
                    </div> <!-- single blog -->
                </div>
            }


        </div> <!-- row -->
    </div> <!-- container -->
</section>
<!--====== Blog Part Ends ======-->



